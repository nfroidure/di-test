{
  "name": "di-test",
  "version": "1.0.0",
  "description": "CLI DI test",
  "main": "index.js",
  "scripts": {
    "compile": "babel --extensions '.ts,.js' src --out-dir=dist",
    "command": "NODE_ENV=${NODE_ENV:-development} babel-node --extensions '.ts,.js' 'bin/run.ts' "
  },
  "author": {
    "name": "Nicolas Froidure",
    "email": "nicolas.froidure@insertafter.com",
    "url": "https://insertafter.com/en/index.html"
  },
  "license": "MIT",
  "dependencies": {
    "@types/argv": "0.0.4",
    "@types/ftp": "^0.3.8",
    "@types/pg": "^7.4.11",
    "argv": "0.0.2",
    "ftp": "^0.3.10",
    "knifecycle": "^5.1.2",
    "pg": "^7.8.0",
    "yerror": "^2.1.3"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/jest": "^23.3.12",
    "@typescript-eslint/parser": "^0.2.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-knifecycle": "^1.0.1",
    "commitizen": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^23.6.0",
    "jsarch": "^2.0.1",
    "jsdoc-to-markdown": "^4.0.1",
    "prettier": "^1.16.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  },
  "contributors": [],
  "engines": {
    "node": ">=8.12.0"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": ".*\\.test\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|jsx)?$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "node": "8.12.0"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "babel-plugin-knifecycle"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "modules": true
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "parser": "@typescript-eslint/parser"
  },
  "prettier": {
    "semi": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  }
}
